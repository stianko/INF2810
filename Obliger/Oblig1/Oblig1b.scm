;; Oppg 2 - Rekursjon over lister og hÃ¸yereordens prosedyrer
;; (a)
(define (length2 items)
  (define (length-iter in out)
    (if (null? in)
	out
	(length-iter (cdr in) (+ 1 out))))
  (length-iter items 0))
;;
;; (b)
(define (rev-list items)
  (define (rev-iter in out)
    (if (null? in)
	out
	(rev-iter (cdr in)
		  (cons (car in) out))))
  (rev-iter items '()))
